/**app.wxss**/
.flexRowBetween{
	display:flex;
	flex-direction:row;
	justify-content:space-between;
}
.flexRowBetween1{
	display:flex;
	flex-direction:row;
}


.loginBtnView {
  margin-top: 0px;
  margin-bottom: 0px;
  padding-bottom: 0px;
}
 
.loginBtn {
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 35px;
  color: #fff;
  background-color:#cba064;
  border: 0.1rpx solid #ccc;
  border-radius: 40rpx;
} 
.flexColumnBetween{
	flex-direction:column;
	justify-content:space-between;
}
.avoidOverflow{
	overflow-x:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
/***********提交按钮***********/
.submit_info{
margin:0rpx 25rpx;
width:700rpx;
height:88rpx;
line-height:88rpx;
text-align:center;
font-size:36rpx;
color:#fff;
background:linear-gradient(to right,#cba064,#cba064);
border-radius:10rpx;margin-top:260rpx;}
/***********navbar***********/
.navbar{
	height:115rpx;
	width:100%;
	position:fixed;
	bottom:0rpx;
	z-index:999;
	background:#fff;
	display:flex;
	justify-content:center;
	border-top:solid 1px #dbdbdb;
	align-items:center;
}
.navbar_item{

	width:25%;
	text-align:center;

}
.navbar .text{
	color:#656565;
}
.navbar .this-text{
	color:#cba064;
}

.navigator-hover {
	background:none;
	/* color:#21c3d4; */
  	color:#cba064;
}