/**app.wxss**/
@import './weui.wxss';
@import './example.wxss';

page {
  font-family: 微软雅黑 ,Cambria, Cochin, Georgia, Times, Times New Roman, serif;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
@font-face {
  font-family: 'iconfont';  /* project id 1658050 */
  src: url('//at.alicdn.com/t/font_1658050_ljy5vofdmx9.eot');
  src: url('//at.alicdn.com/t/font_1658050_ljy5vofdmx9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1658050_ljy5vofdmx9.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1658050_ljy5vofdmx9.woff') format('woff'),
  url('//at.alicdn.com/t/font_1658050_ljy5vofdmx9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1658050_ljy5vofdmx9.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  margin-right: 15rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

